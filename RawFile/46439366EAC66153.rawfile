rebar_hit:
{
	apply_directional_additives:
	{
		apply_ud_scrub_additive:
		{
			apply_lr_scrub_additive:
			{
				base_anim_branch:
				{ 
					HTF_BURI_010_rebar_hit_basepose
				}
				add_lr: additive
				{
					lr_scrub_time_control: scrub1d( timerange="0,1", timevalue=[rebarhit_lr_scrub1d] ) parse_cull(disable) 
					{
						lr_scrub_source:
						{ 
							HTF_BURI_010_rebar_trans_r_to_l	
						}
					}
				}
			}	
			add_ud: additive
			{
				up_scrub_time_control: scrub1d( timerange="0,1", timevalue=[rebarhit_ud_scrub1d] ) parse_cull(disable) 
				{
					ud_scrub_source:
					{ 
						HTF_BURI_010_rebar_trans_u_to_d	
					}
				}
			}
		}
		directional_additive_branch: additive
		{
			add_directional_hits
			{
				directionalblend: blendspace2d( rangex="-1,1", rangey="-1,1", blend_time="0",
				coords="{1,0},{0,0},{0,1},{-1,0},{0,-1}",
				edges="{0,1,2},{1,2,3},{1,3,4},{0,1,4}",
				valuex=[rebarhit_lr_blendspace2d], valuey=[rebarhit_ud_blendspace2d] ) loopsync parse_cull(disable)
				{
					htf_buri_010_rebar_hit_r_player
					htf_buri_010_rebar_hit_c_player
					htf_buri_010_rebar_hit_u_player
					htf_buri_010_rebar_hit_l_player
					htf_buri_010_rebar_hit_d_player
				}
			}
			add_directional_idles
			{
				directionalblend: blendspace2d( rangex="-1,1", rangey="-1,1", blend_time="0",
				coords="{1,0},{0,0},{0,1},{-1,0},{0,-1}",
				edges="{0,1,2},{1,2,3},{1,3,4},{0,1,4}",
				valuex=[rebarhit_lr_blendspace2d], valuey=[rebarhit_ud_blendspace2d] ) loopsync parse_cull(disable)
				{
					htf_buri_010_rebar_idle_r_player
					htf_buri_010_rebar_idle_c_player
					htf_buri_010_rebar_idle_u_player
					htf_buri_010_rebar_idle_l_player
					htf_buri_010_rebar_idle_d_player
				}
			}
		}
	}
}

